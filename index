<script type="text/javascript">

    var lngArray = [];

                lngArray.push({ Caption: "עברית", Url: "https://govextra.gov.il/ministry-of-health/corona/corona-virus/", newWindow: "False" });
                lngArray.push({ Caption: "English", Url: "https://govextra.gov.il/ministry-of-health/corona/corona-virus-en/", newWindow: "False" });
                lngArray.push({ Caption: "Русский", Url: "https://govextra.gov.il/ministry-of-health/corona/corona-virus-ru", newWindow: "False" });
                lngArray.push({ Caption: "عربي", Url: "https://govextra.gov.il/ministry-of-health/corona/corona-virus-ar/", newWindow: "False" });
                lngArray.push({ Caption: "አማርኛ", Url: "https://govextra.gov.il/ministry-of-health/corona/corona-virus-am/", newWindow: "False" });
                lngArray.push({ Caption: "Fran&#231;ais", Url: "https://govextra.gov.il/ministry-of-health/corona/corona-virus-en/guidelines-2/guidelines-french/", newWindow: "False" });
                lngArray.push({ Caption: "Espanol", Url: "https://govextra.gov.il/ministry-of-health/corona/corona-virus-en/guidelines-2/guidelines-spanish/", newWindow: "False" });
                lngArray.push({ Caption: "Українська", Url: "https://govextra.gov.il/ministry-of-health/corona/corona-virus-en/guidelines-2/guidelines-ukranian/", newWindow: "False" });
                lngArray.push({ Caption: "Rom&#226;nesc", Url: "https://govextra.gov.il/ministry-of-health/corona/corona-virus-en/guidelines-2/guidelines-romanian/", newWindow: "False" });
                lngArray.push({ Caption: "ไทย", Url: "https://govextra.gov.il/ministry-of-health/corona/corona-virus-en/guidelines-2/guidelines-thai/", newWindow: "False" });
                lngArray.push({ Caption: "中文", Url: "https://govextra.gov.il/ministry-of-health/corona/corona-virus-en/guidelines-2/guidelines-chinese/", newWindow: "False" });
                lngArray.push({ Caption: "ትግርኛ ፊደል", Url: "https://govextra.gov.il/ministry-of-health/corona/corona-virus-en/guidelines-2/guidelines-tigrinya/", newWindow: "False" });
                lngArray.push({ Caption: "हिन्दी", Url: "https://govextra.gov.il/ministry-of-health/corona/corona-virus-en/guidelines-2/guidelines-hindi/", newWindow: "False" });
                lngArray.push({ Caption: "Wikang Filipino", Url: "https://govextra.gov.il/ministry-of-health/corona/corona-virus-en/guidelines-2/guidelines-filipino/", newWindow: "False" });


    $(window).on('load', function () {

        if (lngArray.length > 0) {



        $('.mobile-menu ul, .mega-mobile-menu ul').ready(function () {


            $(".mobile-menu ul,  .mega-mobile-menu ul").append("<li class='nav-item dropdown'>\
                            <a class='nav-link dropdown-toggle px-2' href='#' id='navbarDropdown' role='button' data-toggle='dropdown' aria-haspopup='true' aria-expanded='false'><span class='minisiteIcon icon-minisite-globe-01 pl-1 pr-2'></span> English <i class='fa fa-angle-right mx-3'></i> <i class='fa fa-angle-down ml-2 d-none'></i> </a>\
                            <ul class='dropdown-menu pt-0' aria-labelledby='navbarDropdown' id='dropdownLangButtonShowMobile'>\
                               <li class= 'dropdown-divider mx-2 mt-0' ></li >\
                            </ul></li> ");



            for (var i = 0; i < lngArray.length; i++) {


                if (lngArray[i].newWindow) {
                    $("#dropdownLangButtonShowMobile").append('<li><a class="dropdown-item px-5" href="' + lngArray[i].Url + '" target="_blank">' + lngArray[i].Caption + '</a></li>');

                }
                else {
                    $("#dropdownLangButtonShowMobile").append('<li><a class="dropdown-item px-5" href="' + lngArray[i].Url + '">' + lngArray[i].Caption + '</a></li>');

                }

            }



            $('.nav-item.dropdown a.dropdown-toggle').on('click', function (e) {


                if (!$(this).next().hasClass('show')) {

                    $(this).parents('.dropdown-menu').first().find('.show').removeClass('show');
                    $(this).parent('.nav-item').siblings().find('.show').removeClass('show');
                    $(this).parent('.nav-item').siblings().find('.primary-palette-color').removeClass('primary-palette-color');

                }


                $(this).toggleClass("primary-palette-color");


            });


        });
    }
    else {
        $(".mobile-nav-button").ready(function () {
            if ($("#navbarCollapse ul li").length < 1 && $("#navbarCollapse").length > 0) {
                $(".mobile-nav-button").hide();
            }
            else if ($(".mst-ancor[data-ancor-show = '1']").length < 1 && $("#navbarCollapse ul li").length < 1) {
                $(".mobile-nav-button").hide();
            }
        });
    }
    });






</script>



