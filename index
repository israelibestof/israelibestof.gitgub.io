        <script>
            $(".megamenu-navigation").ready(function (e) {

                var navList = $(".navbar-nav li.dropdown");
                navList = navList.slice(0, Math.floor(navList.length / 2));
                navList.addClass("start-nav-item");
                //First Level Links
                //Link With childs
                $('a.dropdown-toggle').each(function () {

                    var megaMenuItemCollapseId = $(this).attr("id");

                    if (megaMenuItemCollapseId != undefined) {

                        $(this).bind('mouseenter click focus', function (e) {

                            if ($("div[aria-labelledby=" + megaMenuItemCollapseId + "]").css('display') == 'none') {
                                $("div[aria-labelledby*=dropdown-]").hide();
                                $(".primary-palette-color").removeClass("primary-palette-color");

                                $("div[aria-labelledby=" + megaMenuItemCollapseId + "]").show();
                                $("#" + megaMenuItemCollapseId).addClass('primary-palette-color');
                            }
                            else {

                                $("div[aria-labelledby=" + megaMenuItemCollapseId + "]").hide();
                                $("#" + megaMenuItemCollapseId).removeClass('primary-palette-color');
                            }
                        });
                    }
                    //Link With out Childs
                    else {
                        //Close All
                        $(this).bind('mouseenter click focus', function () {
                            $("div[aria-labelledby*=dropdown-]").hide();
                            $(".primary-palette-color").removeClass("primary-palette-color");
                        });

                    }



                    $(this).closest("nav").bind('mouseleave', function (e) {

                        $("div[aria-labelledby*=dropdown-]").hide();
                        $(".primary-palette-color").removeClass("primary-palette-color");


                    });

                });

                //close on focusout navigation

                $('a.mega-nav-link, a.list-group-item').last().focusout(function () {
                    $("div[aria-labelledby*=dropdown-]").hide();
                });


                //Mobile Mega Menu
                $('.mega-mobile-menu a.dropdown-toggle').on('click', function (e) {

                    //first level close
                    if (!$(this).attr('id')) {

                        $(this).parent('.nav-item').siblings().find('.show').removeClass('show');
                        $(this).parent('.nav-item').siblings().find('.primary-palette-color').removeClass('primary-palette-color');
                        $(this).parent('.nav-item').siblings(".show").removeClass('show');
                    }

                    //second level close
                    if (!$(this).next().hasClass('show')) {

                        $(this).parents('.dropdown-menu').first().find('.show').removeClass('show').removeAttr('style');
                        $(this).parents('.dropdown-menu').first().find('.primary-palette-color').removeClass('primary-palette-color');

                    }

                    var $subMenu = $(this).next('.dropdown-menu');
                    $subMenu.toggleClass('show');
                    $(this).toggleClass('primary-palette-color');


                    //self closing
                    $("a.dropdown-toggle").each(function () {


                        if ($(this).next().is(":not(.show)")) {
                            $(this).removeClass('primary-palette-color');
                        }
                        else {
                            $(this).addClass('primary-palette-color');
                        }


                    });


                    return false;
                });


            });

            
              

</script>


